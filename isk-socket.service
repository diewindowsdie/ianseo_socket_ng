[Unit]
Description=ISK Socket application handles connections between IANSEO and smartphones
After=network.target
After=systemd-user-sessions.service
After=network-online.target

[Service]
User=www-data
Type=simple
WorkingDirectory=/root/ianseo_socket_ng/
ExecStart=/root/ianseo_socket_ng/startSocket.sh
KillSignal=SIGINT
TimeoutSec=30
Restart=on-failure
RestartSec=30
StartLimitInterval=350
StartLimitBurst=10
